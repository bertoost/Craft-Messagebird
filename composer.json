{
  "name": "bertoost/craft-messagebird",
  "description": "Craft CMS plugin for Messagebird messaging features.",
  "type": "craft-plugin",
  "version": "2.0.0",
  "keywords": [
    "craft",
    "craftcms",
    "plugin",
    "craftcms plugin",
    "messagebird",
    "sms",
    "whatsapp"
  ],
  "homepage": "https://github.com/bertoost/Craft-3-Messagebird",
  "license": "MIT",
  "require": {
    "php": "^8.0 || ^8.1",
    "craftcms/cms": "^4.1",
    "messagebird/php-rest-api": "^v3.1"
  },
  "autoload": {
    "psr-4": {
      "bertoost\\messagebird\\": "src/"
    }
  },
  "extra": {
    "name": "Messagebird",
    "handle": "messagebird",
    "hasCpSettings": true,
    "developer": "Bert Oost",
    "developerUrl": "https://bertoost.com",
    "documentationUrl": "https://github.com/bertoost/Craft-3-Messagebird/blob/master/README.md"
  }
}
